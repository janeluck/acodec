<h1>List</h1>
<ul>
{% for item in list %}
  <li>{{ item }}</li>
{% else %}
  <li>This would display if the 'item' collection were empty</li>
{% endfor %}
</ul>
<button onclick="send()">send</button>
<script>
  const ws = new WebSocket('ws://127.0.0.1:7001');
  function send() {
    ws.send(`msg: number${Math.floor(Math.random() * 100)}`);
  }
</script>
